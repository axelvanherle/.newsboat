	"query:All:unread = \"yes\""
	"query:Linux:(tags # \"linux\") and (unread = \"yes\")"
	"query:FOSS:(tags # \"foss\") and (unread = \"yes\")"
	"query:Tech:(tags # \"tech\") and (unread = \"yes\")"
	"query:Blog:(tags # \"blog\") and (unread = \"yes\")"
	"query:Podcasts:(tags # \"podcast\") and (unread = \"yes\")"
	"query:News:(tags # \"news\") and (unread = \"yes\")"
	"---Categorised by date---"
	"query:Today's Articles:age < 1"
	"query:This week's Articles:age < 7"
	"query:This months's Articles:age < 30"
	" "
